{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.chunk": {
			"materials": [
				{
					"*": "Material.chunk"
				}
			],
			"geometry": "geometry.chunk",
			"textures": [
				"Texture.chunk"
			],
			"overlay_color": {
				"r": "(Math.sin(variable.srand + query.life_time * 45))",
				"g": "(Math.sin(variable.srand + query.life_time * 90))",
				"b": "(Math.sin(variable.srand + query.life_time * 180))",
				"a": "0.5"
			},
			"ignore_lighting":true,
			"is_hurt_color":{},
			"on_fire_color":{},
			"light_color_multiplier":1.5
		}
	}
}